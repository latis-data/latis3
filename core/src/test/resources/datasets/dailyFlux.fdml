<?xml version="1.0" encoding="UTF-8"?>

<dataset id="dailyFlux"
         title="Test Flux Dataset"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://latis-data.io/schemas/1.0/fdml-with-columnar-adapter.xsd">

  <source uri="data/dailyFlux.txt"/>

  <adapter class="latis.input.ColumnarAdapter"
           skipLines="1"
           delimiter=","
           columns="0;1;2;3"
           />

  <function>
    <tuple id="time">
        <scalar id="year" units="yyyy" type="int"/>
        <tuple id="time2">
            <scalar id="month" units="MM" type="int"/>
            <scalar id="day" units="dd" type="int"/>
        </tuple>
    </tuple>
    <scalar id="ra" type="int"/>
  </function>

</dataset>
